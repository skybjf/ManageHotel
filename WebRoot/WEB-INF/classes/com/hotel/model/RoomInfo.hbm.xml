<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.hotel.model.RoomInfo" table="room_info" catalog="hotel">
		<id name="id" type="java.lang.Integer">
			<column name="id" />
			<generator class="identity" />
		</id>
		<property name="roomTypeId" type="java.lang.String" not-null="true">
			<column name="type_id">
			</column>
		</property>
		<property name="state" type="java.lang.String" not-null="true">
			<column name="state" length="50">
			</column>
		</property>
		<property name="bookTime" type="java.lang.String" not-null="true">
			<column name="book_time" length="50">
			</column>
		</property>
		<property name="delMark" type="java.lang.String" not-null="true">
			<column name="delmark" length="50">
			</column>
		</property>
	</class>
</hibernate-mapping>
